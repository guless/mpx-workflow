{
    "name": "mpx-workflow",
    "version": "1.0.0",
    "scripts": {
        "watch": "npm run build -- --watch --env.mode=development",
        "build": "Webpack --config=./toolkit/webpack.config.js --progress --env.mode=production",
        "watch:test": "npm run watch -- --env.target=test",
        "watch:beta": "npm run watch -- --env.target=beta",
        "watch:prod": "npm run watch -- --env.target=prod",
        "build:test": "npm run build -- --env.target=test",
        "build:beta": "npm run build -- --env.target=beta",
        "build:prod": "npm run build -- --env.target=prod",
        "dev": "npm run watch:test",
        "start": "npm run watch:test"
    },
    "devDependencies": {
        "@types/node": "^10.5.2",
        "@types/webpack": "^4.4.5",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "clean-webpack-plugin": "^0.1.19",
        "copy-webpack-plugin": "^4.5.2",
        "css-loader": "^0.28.11",
        "extract-loader": "^2.0.1",
        "form-data": "^2.3.2",
        "gs-events": "^1.0.11",
        "html-loader": "^0.5.5",
        "mkdirp": "^0.5.1",
        "mpx-webpack-plugin": "^1.0.9",
        "node-fetch": "^2.1.2",
        "node-sass": "^4.9.1",
        "postcss-loader": "^2.1.5",
        "preprocess-loader": "^0.2.2",
        "resolve-url-loader": "^2.3.0",
        "sass-loader": "^7.0.3",
        "webpack": "^4.15.1",
        "webpack-cli": "^3.0.8"
    }
}
